<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.incognito.hangman">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_crash_dummy_2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_crash_dummy_2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManagePlayersActivity" />
        <activity android:name=".SetupOnePlayerGameActivity" />
        <activity android:name=".SetupTwoPlayerGameActivity" />
        <activity android:name=".OnePlayerGame" />
        <activity android:name=".TwoPlayerGame" />
    </application>

</manifest>